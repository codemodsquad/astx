{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Tests",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/.bin/mocha",
        "${workspaceRoot}/test/configure.js",
        "${workspaceRoot}/test/**/*.ts"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug -f",
      "program": "${workspaceFolder}/src/cli/index.ts",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "runtimeArgs": ["--nolazy", "--extensions", ".ts"],
      "args": [
        "-f",
        "function $fn<$_params>($_args): CompiledReplacement<$_f> { $_body }",
        "src/compileReplacement/Capture.ts"
      ],
      "env": {
        "BABEL_ENV": "development"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug mqtt-broker",
      "program": "${workspaceFolder}/src/cli/index.ts",
      "cwd": "/Users/andy/github/mqtt-broker",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "runtimeArgs": ["--nolazy", "--extensions", ".ts"],
      "args": ["test/integration/SessionStoreTests.js"],
      "env": {
        "BABEL_ENV": "development"
      }
    }
  ]
}
